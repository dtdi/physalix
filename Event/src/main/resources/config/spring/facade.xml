<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean name="event.facade" class="hsa.awp.event.facade.EventFacade">
        <property name="categoryDao" ref="event.dao.category"></property>
        <property name="eventDao" ref="event.dao.event"></property>
        <property name="subjectDao" ref="event.dao.subject"></property>
        <property name="examDao" ref="event.dao.exam"></property>
        <property name="termDao" ref="event.dao.term"></property>
        <property name="occurrenceDao" ref="event.dao.occurence"></property>
        <property name="timetableDao" ref="event.dao.timeTable"></property>
    </bean>

</beans>
